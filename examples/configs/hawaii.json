{
  "region": "hawaii",
  "description": "Standard configuration for Hawaii earthquake data processing",
  "geographic_bounds": {
    "minlatitude": 18.0,
    "maxlatitude": 23.0,
    "minlongitude": -161.0,
    "maxlongitude": -154.0
  },
  "velocity_model": {
    "type": "simple",
    "description": "Simple velocity model for Hawaii volcanic region",
    "vp": 5.8,
    "vs": 3.35,
    "vp_vs_ratio": 1.73
  },
  "processing": {
    "phasenet": {
      "model": "phasenet_original",
      "batch_size": 1,
      "threshold_p": 0.25,
      "threshold_s": 0.25,
      "input_format": "mseed",
      "sampling_rate": 100,
      "window_length": 30
    },
    "gamma": {
      "vel": {"P": 5.8, "S": 3.35},
      "dims": ["x_km", "y_km", "z_km"],
      "use_amplitude": true,
      "method": "BGMM",
      "oversample_factor": 4,
      "bfgs_bounds": [
        [null, null], [null, null], [0, 50],
        [null, null], [0, null], [0, null], [0, null]
      ],
      "dbscan_eps": 0.3,
      "dbscan_min_samples": 3,
      "min_picks_per_eq": 6,
      "max_sigma11": 1.5,
      "max_sigma22": 0.8,
      "max_sigma12": 0.8
    },
    "adloc": {
      "vel": {"P": 5.8, "S": 3.35},
      "use_amplitude": true,
      "method": "BFGS",
      "use_station_term": true,
      "cc_max": 0.6,
      "method_magnitude": "ML"
    }
  },
  "data_sources": {
    "waveforms": {
      "provider": "HVO",
      "format": "mseed",
      "channels": ["HH*", "BH*", "EH*"]
    },
    "stations": {
      "provider": "HVO",
      "format": "stationxml"
    },
    "catalog": {
      "provider": "HVO",
      "format": "quakeml"
    }
  },
  "output": {
    "format": "csv",
    "compression": "gzip",
    "directory_structure": {
      "picks": "picks",
      "events": "events",
      "stations": "stations",
      "figures": "figures"
    }
  },
  "quality_control": {
    "min_magnitude": 0.5,
    "max_depth": 50.0,
    "min_picks_per_event": 6,
    "max_residual_time": 2.5,
    "max_residual_amplitude": 2.0
  }
}